{
    "name": "@collab/database",
    "version": "1.0.0",
    "description": "Prisma database package for AI Real-Time Content Collaboration Platform",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "db:generate": "ts-node scripts/db-runner.ts generate",
        "db:push": "ts-node scripts/db-runner.ts db push",
        "db:migrate": "ts-node scripts/db-runner.ts migrate dev",
        "db:migrate:deploy": "ts-node scripts/db-runner.ts migrate deploy",
        "db:migrate:reset": "ts-node scripts/db-runner.ts migrate reset",
        "db:studio": "ts-node scripts/db-runner.ts studio",
        "build": "tsc",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@prisma/client": "5.22.0",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "prisma": "5.22.0",
        "typescript": "^5.4.5",
        "ts-node": "^10.9.2",
        "@types/node": "^20.12.7"
    },
    "license": "UNLICENSED"
}